generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type EventsListDateRange {
  from String
  to   String
}

type EventsListProblems {
  grade Int
  name  String
}

model events_list {
  id        String               @id @default(auto()) @map("_id") @db.ObjectId
  angle     Int
  dateRange EventsListDateRange
  eventName String
  problems  EventsListProblems[]
}

type UserListProblems {
  grade Int?
  name  String?
  top   Boolean
}

type UserEventsList {
  id        String
  eventName String
  problems  UserListProblems[]
}

model users_list {
  id          String           @id @default(auto()) @map("_id") @db.ObjectId
  email       String           @unique
  password    String
  role        String
  username    String           @unique
  gender      String
  events_list UserEventsList[]
}
